<section>
    <div class="container mt-5">
        <h3 class="text-center">ALL USERS</h3>
        <table class="table mt-5 pt-5" id="table">
            <thead>
                <tr>

                    <th scope="col">User Name</th>
                    <th scope="col">E-Mail</th>

                </tr>
            </thead>
            <tbody>
                {{#each users}}
                <tr>
                    <td>{{this.name}}</td>
                    <td>{{this.email}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</section>